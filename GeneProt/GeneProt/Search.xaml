                                <ComboBox Name="geneMapping_rel" Grid.Row="5" Grid.Column="1" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Loaded="ComboBoxMapping_Loaded" SelectionChanged="ComboBoxMapping_SelectionChanged"/>
