                                    <Button Grid.Row="0" Grid.Column="3" Content="Limpar" Style="{DynamicResource SquareButtonStyle}" HorizontalAlignment="Left" Margin="0,10,5,0" VerticalAlignment="Top"  Click="GeneM_Clear"/>
                                    <Button Grid.Row="0" Grid.Column="4" Content="Pesquisar" Style="{DynamicResource SquareButtonStyle}" HorizontalAlignment="Left" Margin="0,10,5,0" VerticalAlignment="Top" Click="GeneMapping_Search"/>
