                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Button  Grid.Column="0" Content="Get Relation" Style="{DynamicResource SquareButtonStyle}" HorizontalAlignment="Center" Margin="0,10,5,0" VerticalAlignment="Top"  />
                                    <Button  Grid.Column="1" Content="Normal View" Style="{DynamicResource SquareButtonStyle}" HorizontalAlignment="Center" Margin="0,10,5,0" VerticalAlignment="Top" Click="GeneM_reset"/>
                                </Grid>
                            </Grid>
                        </StackPanel>
                    </ScrollViewer>
                </Grid>
            </TabItem>
